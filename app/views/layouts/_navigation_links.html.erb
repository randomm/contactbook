<%# add navigation links to this file %>
<% if current_user %>
    <li><%=link_to current_user.username, current_user%></li>
    <li><%=link_to "Sign out", signout_path, method: :delete%></li>
<%else%>
    <li><%=link_to "Sign in", signin_path%></li>
    <li><%= link_to "Register", signup_path%></li>
<%end%>
